msgid "Show answers while voting"
msgstr "Ցույց տալ պատասխանները քվեարկության պահին"

#: poll.py:832
msgid "Remember last vote"
msgstr "Հիշել վերջին քվեն"

#: poll.py:838
msgid "Play a sound when make a vote"
msgstr "Քվեարկելիս հնչյունային ազդանշան տալ"

#: poll.py:845
msgid "Use image in answer"
msgstr "Պատասխանում պատկեր օգտագործել"

#: poll.py:850
msgid "Image Size: "
msgstr "Պատկերի չափսը. "

#: poll.py:872
msgid "Save"
msgstr "պահել"

#: poll.py:929
msgid "The settings have been saved"
msgstr "Չափանիշները պահվել են"

#: poll.py:964
msgid "Favorite Color"
msgstr "Նախընտրած գույնը"

#: poll.py:966
msgid "What is your favorite color?"
msgstr "Ո՞րն է Ձեր նախընտրած գույնը"

#: poll.py:970
msgid "Orange"
msgstr "Նարնջագույն"

#: poll.py:971
msgid "None of the above"
msgstr "Վերոհիշյալներից ոչ մեկը"

#: poll.py:1037
#, python-format
msgid "Somebody voted on %s"
msgstr "Ինչ-որ մեկը քվեարկել է %s-ի կապակցությամբ"

#: poll.py:1056
msgid "Close Lessons"
msgstr "Փակել Դասերը"

#: poll.py:1144 poll.py:1173
msgid "Joined"
msgstr "Միացավ"

#: poll.py:1177
msgid "Left"
msgstr "Լքեց"

#: poll.py:1549 poll.py:1613
msgid "New Poll"
msgstr "Նոր հարցախույզ"

